pipeline{
	agent any;
	stages{
		stage('Set up')
		{
			steps {
				cleanWs();
				sh 'echo setting up my workspace'
			}
		}
		stage('Checkout SCM')
		{
			steps {
				checkout scm
			}
		}
		stage('Compile Code')
		{
			steps {
				sh 'chmod +x file1'
			}
		}
		stage('Testing')
		{
			steps {
				sh 'cat file2.txt'
			}
		}
	}

}
